# This profile is used for building debug versions of libraries on the raspberry pi
include(common-default-options)
include(bullseye-gcc10-armv7hf)

[settings]
# The libraries below are only built in Release mode
fontconfig:build_type=Release
freetype:build_type=Release
qt:build_type=Release
qwt:build_type=Release
rstatistics:build_type=Release

[options]

[build_requires]

[env]
