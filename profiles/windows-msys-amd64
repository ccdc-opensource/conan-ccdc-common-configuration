# This profile is used for building on Windows, with msys. Used only for some packages
include(common-default-options)

[settings]
arch=x86_64
arch_build=x86_64

os=Windows
os.subsystem=msys2
os_build=Windows

compiler=gcc
compiler.version=10
compiler.libcxx=libstdc++11
compiler.cppstd=17

[options]

# this option is not available on windows
# mariadb-connector-c:with_iconv=False

# See the more extensive comment in common-default-options
gtest:shared=False

# libelf can not be built as shared library on non linux or windows platforms
libelf:shared=False

[build_requires]

[env]
