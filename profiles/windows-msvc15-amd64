# This profile is used for building on Windows, with visual studio 2017
include(default)
include(common-default-options)

[settings]
arch=x86_64
arch_build=x86_64
compiler=Visual Studio
compiler.version=15
compiler.toolset=v141
compiler.cppstd=17

os=Windows
os_build=Windows
libtool:compiler=Visual Studio
libtool:compiler.version=15
libtool:compiler.toolset=v141

[options]
libcurl:with_openssl=False
libcurl:with_winssl=True

[build_requires]

[env]
